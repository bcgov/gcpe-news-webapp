@using Gov.News.Website.Models
@model NewsletterViewModel
<div class="newsletters">
    <div class="access">
        <ul>
            <li aria-label="Skip to main content">
                <a href="#main-content-newsletters">Skip to main content</a>
            </li>
            <li aria-label="Skip to footer">
                <a href="#footer" onclick="window.scrollTo(0,0);">Skip to footer</a>
            </li>
        </ul>
    </div>
    <div class="home-features" role="complementary" id="sidebar">
        <partial name="ConnectSocialMedia" />
        <partial name="ResourceLinks" />
        <vc:top-sectors />
        </div>

        <!--googleon: all-->
    <div class="home-body" id="main-content-newsletters" role="main">
        <h2 style="border-bottom: none !important;"><a href=@string.Format("../{0}", Model.Newsletter.Key)>@Model.Newsletter.Name</a></h2>
        <h3 style="border-bottom: none !important; margin-bottom: 20px !important;">Edition: @Model.Edition.Name</h3>
        <div class="clear">
            @Html.Raw(Model.Edition.HtmlBody)
        </div>
        @*</div>*@
    </div>
        <!--googleoff: all-->
    <div class="clear"></div>
</div>

